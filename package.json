{
  "name": "webpack-starter-kit",
  "version": "1.0.0",
  "description": "Kit de inicio para proyectos con Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --color --progress --stdin --config ./webpack.config.dev.babel.js",
    "dev:local": "webpack -d --watch --colors --config ./webpack.config.dev.babel.js",
    "dev:external": "webpack -d --watch --colors --config ./webpack.config.dev.babel.js",
    "build": "",
    "dash": "webpack-dashboard -c -- webpack-dev-server --config ./webpack.config.dev.babel.js --colors",
    "clean": "rimraf ./public/*.png ./public/*.json ./public/*.html ./public/*.xml ./public/*.ico ./public/.cache ./public/*.webapp ./public/*.js ./public/assets/*",
    "prod": "webpack --config ./webpack.config.prod.babel.js -p --env.prod --colors"
  },
  "browserslist": [
    "> 1%",
    "ie > 10",
    "Last 10 versions"
  ],
  "author": "Samuel R.",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "favicons-webpack-plugin-cesco": "0.0.6",
    "file-loader": "^1.1.5",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "os": "^0.1.1",
    "postcss-loader": "^2.0.8",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "resolve-url-loader": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dotenv-extended": "^2.0.1",
    "font-awesome": "^4.7.0",
    "normalize.css": "^7.0.0"
  }
}
